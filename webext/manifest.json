{
    "manifest_version": 3,
    "name": "Lift Web Restrictions",
    "description": "A simple, easy to use extension that removes most web annoyances: ads, cookie banners, etc. It also enhances some online games by introducing mods into them, and more!",
    "version": "0.0.1",
    "icons": {
        "16": "logo/sw16.png",
        "48": "logo/sw48.png",
        "128": "logo/sw128.png"
    },
    "permissions": [],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "web_accessible_resources": [
        {
          "resources": ["modules/script.js"],
          "matches": ["*://*/*"]
        }
    ],     
    "content_scripts": [{
        "js": [
            "modules/transfer.js"
        ],
        "matches": ["*://*/*"],
        "run-at": "document-start"
    }]
}
