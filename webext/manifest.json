{
    "manifest_version": 3,
    "name": "Lift Web Restrictions",
    "description": "A simple, easy to use extension that removes most web annoyances: ads, cookie banners, etc. It also enhances some online games by introducing mods into them, and more!",
    "version": "0.0.1",
    "icons": {
        "16": "logo/sw16.png",
        "48": "logo/sw48.png",
        "128": "logo/sw128.png"
    },
    "permissions": [],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [{
        "js": [
            "modules/script.js"
        ],
        "matches": [
            "*://*.discord.com/app",
            "*://*.discord.com/channels/*",
            "*://*.discord.com/login",
            "*://facebook.com/*",
            "*://*.facebook.com/*",
            "*://starve.io/*",
            "*://classroom.google.com/*",
            "*://*.moomoo.io/*",
            "*://moomoo.io/*",
            "*://sandbox.moomoo.io/*",
            "*://dev.moomoo.io/*",
            "*://linkvertise.com/*",
            "*://www.google.com/recaptcha/api2/*",
            "*://diep.io/*",
            "*://www.baidu.com/*",
            "*://baidu.com/*",
            "*://m.baidu.com/*"
        ],
        "run-at": "document-start"
    }]
}
